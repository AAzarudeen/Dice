<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="100dp"
        android:height="100dp">
        <shape  android:shape="rectangle">
            <corners android:radius="15dp"/>
            <solid android:color="@color/mediumGreen"/>
        </shape>
    </item>
    <item android:width="25dp"
        android:height="25dp"
        android:top="20dp"
        android:left="15dp">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:width="25dp"
        android:height="25dp"
        android:top="20dp"
        android:left="60dp">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:width="25dp"
        android:height="25dp"
        android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:width="25dp"
        android:height="25dp"
        android:top="60dp"
        android:left="15dp">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:width="25dp"
        android:height="25dp"
        android:top="60dp"
        android:left="60dp">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>
</layer-list>